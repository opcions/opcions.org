{% if name is defined %}
{% if not file_exists %}
services:
{% endif %}
  {{ name | lower }}:
    class: Drupal\{{module}}\{{ class }}
    arguments: [{{ servicesAsParametersKeys(services)|join(', ') }}]
{% if tags is defined %}
    tags:
      - { {{ tagsAsArray(tags)|join(', ') }} }
{% endif %}
{% endif %}

